{
  "name": "shanetaylor",
  "version": "5.2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:TaylorShane/shanetaylor.dev.git"
  },
  "author": {
    "name": "Shane Taylor"
  },
  "homepage": "https://shanetaylor.dev/",
  "bugs": "https://github.com/TaylorShane/shanetaylor.dev/issues",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "clean": "rimraf dist",
    "commitlint": "commitlint --from=origin/master",
    "e2e": "ng e2e",
    "eclint": "eclint check src/*/*",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && prettier --write \"src/**/*.{ts,html,scss}\"",
    "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{ts,sass,html}\"",
    "lint:sass": "stylelint **.scss",
    "lint:editorconfig": "eclint check src/**",
    "lint:all": "npm run lint && npm run lint:sass && npm run lint:editorconfig",
    "prepare": "husky install",
    "release": "npm version patch && standard-version && git push --follow-tags",
    "sass-lint": "sass-lint --config .sass-lint.yml --verbose",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadlessCI"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~20.3.10",
    "@angular/common": "20.3.10",
    "@angular/compiler": "~20.3.10",
    "@angular/core": "~20.3.10",
    "@angular/forms": "~20.3.10",
    "@angular/localize": "^20.3.10",
    "@angular/platform-browser": "~20.3.10",
    "@angular/platform-browser-dynamic": "~20.3.10",
    "@angular/router": "~20.3.10",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "echarts": "^5.5.0",
    "ngx-bootstrap": "^20.0.2",
    "ngx-echarts": "^20.0.2",
    "rxjs": "^7.8.1",
    "stylelint": "^16.25.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "tslib": "^2.6.2",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.3.9",
    "@angular-eslint/builder": "20.5.1",
    "@angular-eslint/eslint-plugin": "^20.5.1",
    "@angular-eslint/eslint-plugin-template": "^20.5.1",
    "@angular-eslint/schematics": "20.5.1",
    "@angular-eslint/template-parser": "20.5.1",
    "@angular/cli": "20.3.9",
    "@angular/compiler-cli": "~20.3.10",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/jasmine": "5.1.4",
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eclint": "^2.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "^9.0.11",
    "jasmine-core": "5.1.2",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "protractor": "~7.0.0",
    "puppeteer": "22.15.0",
    "sass-lint": "^1.13.1",
    "standard-version": "^9.5.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.21.3"
  },
  "lint-staged": {
    "**/*.{html,ts}": [
      "npm run lint",
      "npm run lint:editorconfig"
    ],
    "**/*.scss": [
      "npm run lint:sass"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run commitlint",
      "pre-commit": "npm run lint-staged && npm run test && npm run prettier:check"
    }
  }
}
