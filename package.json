{
  "name": "shanetaylor",
  "version": "5.2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:TaylorShane/shanetaylor.dev.git"
  },
  "author": {
    "name": "Shane Taylor"
  },
  "homepage": "https://shanetaylor.dev/",
  "bugs": "https://github.com/TaylorShane/shanetaylor.dev/issues",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "clean": "rimraf dist",
    "commitlint": "commitlint --from=origin/master",
    "e2e": "ng e2e",
    "eclint": "eclint check src/*/*",
    "lint": "ng lint && sass lint",
    "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{ts,sass,html}\"",
    "prepare": "husky install",
    "release": "yarn version patch && standard-version && git push --follow-tags",
    "sass-lint": "sass-lint --config .sass-lint.yml --verbose",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadlessCI"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.2.1",
    "@angular/common": "17.2.1",
    "@angular/compiler": "~17.2.1",
    "@angular/core": "~17.2.1",
    "@angular/forms": "~17.2.1",
    "@angular/localize": "^17.2.1",
    "@angular/platform-browser": "~17.2.1",
    "@angular/platform-browser-dynamic": "~17.2.1",
    "@angular/router": "~17.2.1",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "echarts": "^5.4.0",
    "ngx-bootstrap": "^10.2.0",
    "ngx-echarts": "16.0.0",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.1",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.2.0",
    "@angular-eslint/builder": "16",
    "@angular-eslint/eslint-plugin": "^16.0.3",
    "@angular-eslint/eslint-plugin-template": "^16.0.3",
    "@angular-eslint/schematics": "16",
    "@angular-eslint/template-parser": "16",
    "@angular/cli": "17.2.0",
    "@angular/compiler-cli": "~17.2.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/jasmine": "4.3.5",
    "@types/node": "20.4.1",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "eclint": "^2.8.1",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "46.4.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "^8.0.2",
    "jasmine-core": "5.0.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "lint-staged": "^13.0.4",
    "prettier": "3.0.0",
    "protractor": "~7.0.0",
    "puppeteer": "20.8.0",
    "sass-lint": "^1.13.1",
    "standard-version": "^9.5.0",
    "ts-node": "10.9.1",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "**/*.{html,ts}": [
      "yarn eslint",
      "yarn eclint"
    ],
    "**/*.scss": [
      "yarn sass-lint"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "yarn commitlint",
      "pre-commit": "yarn lint-staged && yarn test && yarn prettier:check"
    }
  }
}
