{
  "name": "shanetaylor",
  "version": "6.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:TaylorShane/shanetaylor.dev.git"
  },
  "author": {
    "name": "Shane Taylor"
  },
  "homepage": "https://shanetaylor.dev/",
  "bugs": "https://github.com/TaylorShane/shanetaylor.dev/issues",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5150",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "clean": "rimraf dist",
    "commitlint": "commitlint --from=origin/master",
    "e2e": "ng e2e",
    "eclint": "eclint check src/*/*",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && prettier --write \"src/**/*.{ts,html,scss}\"",
    "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{ts,sass,html}\"",
    "lint:sass": "stylelint **.scss",
    "lint:editorconfig": "eclint check src/**",
    "lint:all": "npm run lint && npm run lint:sass && npm run lint:editorconfig",
    "prepare": "husky install",
    "release": "npm version patch && standard-version && git push --follow-tags",
    "sass-lint": "sass-lint --config .sass-lint.yml --verbose",
    "test": "ng test --code-coverage",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadlessCI"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~21.0.3",
    "@angular/common": "21.0.3",
    "@angular/compiler": "~21.0.3",
    "@angular/core": "~21.0.3",
    "@angular/forms": "~21.0.3",
    "@angular/localize": "^21.0.3",
    "@angular/platform-browser": "~21.0.3",
    "@angular/platform-browser-dynamic": "~21.0.3",
    "@angular/router": "~21.0.3",
    "@ngx-env/builder": "^21.0.1",
    "bootstrap": "^5.3.2",
    "echarts": "^6.0.0",
    "ngx-echarts": "^21.0.0",
    "rxjs": "^7.8.1",
    "stylelint": "^16.26.1",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "tslib": "^2.6.2",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.3",
    "@angular-eslint/builder": "21.1.0",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/schematics": "21.1.0",
    "@angular-eslint/template-parser": "21.1.0",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "~21.0.3",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/jasmine": "^5.1.13",
    "@types/node": "^25.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eclint": "^2.8.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "^9.0.11",
    "jasmine-core": "^5.13.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "protractor": "~7.0.0",
    "puppeteer": "^24.32.1",
    "sass-lint": "^1.13.1",
    "standard-version": "^9.5.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.21.3"
  },
  "lint-staged": {
    "**/*.{html,ts}": [
      "npm run lint",
      "npm run lint:editorconfig"
    ],
    "**/*.scss": [
      "npm run lint:sass"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run commitlint",
      "pre-commit": "npm run lint-staged && npm run test && npm run prettier:check"
    }
  }
}
